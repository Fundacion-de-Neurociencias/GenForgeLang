   "id": "intro-cell",
   "metadata": {},
   "source": [
    "# 🧬 GeneForgeLang Parser Notebook\n",
    "This notebook allows you to input GeneForgeLang phrases and parse them syntactically using the updated grammar and metadata blocks."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "load-grammar",
   "metadata": {},
   "outputs": [],
   "source": [
    "import json\n",
    "import re\n",
    "\n",
    "with open(\"geneforge_grammar.json\", \"r\") as f:\n",
    "    grammar = json.load(f)\n",
    "\n",
    "# Compile regex patterns\n",
    "regex_prefix = re.compile(r\"^([~:\\\^\\*!])([drp]):\")\n",
    "regex_modules = re.compile(r\"(Dom|Mot|TF|Ctrl|PTM)\\(([^)]+)\\)\")\n",
    "regex_ptm = re.compile(r\"([A-Z])\\*([A-Za-z]+)@(\\d+)\")\n",
    "regex_compact_ptm = re.compile(r\"\\*([A-Za-z]+)([A-Z])@(\\d+)\")\n",
    "regex_mut = re.compile(r\"\\[MUT:([A-Z])>([A-Z])@(\\d+)]\")\n",
    "regex_mut_prov = re.compile(r\"\\[MUT:(PAT|MAT|SOM|GER):([A-Z])>([A-Z])@(\\d+)]\")\n",
    "regex_edit = re.compile(r\"EDIT:(Base|Prime|ARCUS)\\(([^)]+)\\)(\\{[^}]+\\})?\")\n",
    "regex_deliv = re.compile(r\"DELIV\\(([^@)]+)@([^\\)]+)\\)\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "parser-function",
   "metadata": {},
   "outputs": [],
   "source": [
    "def parse_metadata_block(block):\n",
    "    metadata = {}\n",
    "    block = block.strip(\"{} \")\n",
    "    for pair in block.split(','):\n",
    "        if '=' in pair:\n",
    "            key, val = pair.strip().split('=')\n",
    "            metadata[key.strip()] = val.strip()\n",
    "    return metadata\n",
    "\n",
    "def parse_geneforge_line(line):\n",
    "    output = {\n",
    "        'valid': False,\n",
    "        'structure': None,\n",
    "        'molecule': None,\n",
    "        'modules': [],\n",
    "        'ptms': [],\n",
    "        'mutations': [],\n",
    "        'edits': [],\n",
    "        'delivery': None,\n",
    "        'errors': []\n",
    "    }\n",
    "\n",
    "    prefix_match = regex_prefix.match(line)\n",
    "    if not prefix_match:\n",
    "        output['errors'].append('Invalid or missing structural/molecular prefix.')\n",
    "        return output\n",
    "\n",
    "    structure_symbol, molecule_code = prefix_match.groups()\n",
    "    output['structure'] = grammar['structures'].get(structure_symbol)\n",
    "    output['molecule'] = grammar['molecules'].get(molecule_code)\n",
    "\n",
    "    output['modules'] = [{\"type\": m, \"value\": v} for m, v in regex_modules.findall(line)]\n",
    "\n",
    "    for r, t, pos in regex_ptm.findall(line):\n",
    "        output['ptms'].append({\n",
    "            'notation': 'ProForma',\n",
    "            'residue': r,\n",
    "            'modification': t,\n",
    "            'position': int(pos)\n",
    "        })\n",
    "\n",
    "    for t, r, pos in regex_compact_ptm.findall(line):\n",
    "        output['ptms'].append({\n",
    "            'notation': 'GFL',\n",
    "            'residue': r,\n",
    "            'modification': t,\n",
    "            'position': int(pos)\n",
    "        })\n",
    "\n",
    "    for origin, f, t, pos in regex_mut_prov.findall(line):\n",
    "        output['mutations'].append({\n",
    "            'origin': origin, 'from': f, 'to': t, 'position': int(pos)\n",
    "        })\n",
    "\n",
    "    for f, t, pos in regex_mut.findall(line):\n",
    "        output['mutations'].append({\n",
    "            'from': f, 'to': t, 'position': int(pos)\n",
    "        })\n",
    "\n",
    "    for kind, op, meta in regex_edit.findall(line):\n",
    "        output['edits'].append({\n",
    "            'type': kind,\n",
    "            'operation': op,\n",
    "            'metadata': parse_metadata_block(meta) if meta else {}\n",
    "        })\n",
    "\n",
    "    delivery_match = regex_deliv.search(line)\n",
    "    if delivery_match:\n",
    "        output['delivery'] = {\n",
    "            'vector': delivery_match.group(1),\n",
    "            'route': delivery_match.group(2)\n",
    "        }\n",
    "\n",
    "    output['valid'] = True\n",
    "    return output"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "test-input",
   "metadata": {},
