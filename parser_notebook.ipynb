   "source": [
    "# ✍️ Test phrase using new grammar features\n",
    "test_input = \"~d:[TATA]CPS1[MUT:PAT:A>G@1001]EDIT:Base(A→G@1001){efficacy=partial, cells=liver}DELIV(mRNA+LNP@IV)\"\n",
    "\n",
    "# Parse the input\n",
    "parsed_result = parse_geneforge_line(test_input)\n",
    "parsed_result"
   ]
